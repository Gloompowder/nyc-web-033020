<div class="container">
  <h1>New GOAT</h1>

  <%= form_for @goat do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :age %>
    <%= f.number_field :age %>

    <%= f.label :occupation %>
    <%= f.text_field :occupation %>
    
    <%= f.submit %>
  <% end %>
</div>


