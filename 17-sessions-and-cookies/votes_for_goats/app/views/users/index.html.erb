<div class='container'>
  <h1>User Index</h1>

  <div class="index-container">

    <% @users.each do |user| %>
      <div class='index-tile'>
        <h3><%= link_to user.name, user_path(user.id) %></h3>
         <img class='index-img' src="<%= user.img_url %>" alt="<%user.name%>">
      </div>
    <% end %>
    
  </div>

</div>